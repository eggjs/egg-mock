{
  "name": "egg-mock",
  "version": "0.0.4",
  "eggPlugin": {
    "name": "egg-mock"
  },
  "description": "mock server for egg",
  "main": "index.js",
  "files": [
    "index.js",
    "app",
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib index.js test/*.test.js",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "TEST_TIMEOUT=40000 egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "autod": "autod -w --prefix '^'",
    "contributors": "ali-contributors"
  },
  "dependencies": {
    "coffee": "^3.2.4",
    "debug": "^2.2.0",
    "egg-utils": "^1.0.0",
    "get-ready": "^1.0.0",
    "is-type-of": "^1.0.0",
    "merge-descriptors": "^1.0.1",
    "mm": "^1.5.0",
    "rimraf": "^2.5.3"
  },
  "devDependencies": {
    "autod": "*",
    "egg-bin": "^1.0.0",
    "egg-ci": "^1.0.2",
    "eslint": "2",
    "eslint-config-egg": "2",
    "should": "^8.3.0",
    "supertest": "^1.2.0"
  },
  "ci": {
    "type": "travis, appveyor",
    "npminstall": true,
    "version": [
      "4",
      "6"
    ],
    "command": "ci"
  },
  "homepage": "https://github.com/eggjs/egg-mock",
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/egg-mock.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg-mock/issues"
  },
  "keywords": [
    "egg",
    "mock"
  ],
  "engines": {
    "node": ">= 4.0.0"
  },
  "author": "popomore <sakura9515@gmail.com>"
}
