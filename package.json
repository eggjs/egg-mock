{
  "name": "egg-mock",
  "version": "1.0.0",
  "eggPlugin": {
    "name": "egg-mock"
  },
  "description": "mock server for egg",
  "main": "index.js",
  "files": [
    "index.js",
    "app",
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib app index.js test/*.test.js",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test -r intelli-espower-loader",
    "cov": "egg-bin cov -r intelli-espower-loader",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "autod-china": "autod --registry=https://registry.npm.taobao.org",
    "contributors": "ali-contributors"
  },
  "dependencies": {
    "coffee": "^3.3.0",
    "debug": "^2.2.0",
    "get-ready": "^1.0.0",
    "is-type-of": "^1.0.0",
    "ko-sleep": "^1.0.2",
    "merge-descriptors": "^1.0.1",
    "mm": "^2.0.0",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^0.4.0",
    "egg-bin": "^1.6.0",
    "egg-ci": "^1.0.3",
    "eslint": "^3.9.1",
    "eslint-config-egg": "^3.1.0",
    "intelli-espower-loader": "^1.0.1",
    "ko-sleep": "^1.0.2",
    "pedding": "^1.0.0",
    "power-assert": "^1.4.1",
    "supertest": "^2.0.1"
  },
  "ci": {
    "type": "travis, appveyor",
    "npminstall": true,
    "version": [
      "4",
      "6"
    ],
    "command": "ci"
  },
  "homepage": "https://github.com/eggjs/egg-mock",
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/egg-mock.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg-mock/issues"
  },
  "keywords": [
    "egg",
    "mock"
  ],
  "engines": {
    "node": ">= 4.0.0"
  },
  "author": "popomore <sakura9515@gmail.com>"
}
